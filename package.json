{
  "name": "netlify-cli",
  "version": "0.0.0",
  "description": "CLI to interact with Netlify",
  "author": "davidwells @DavidWells",
  "bin": {
    "ntl": "./bin/run",
    "netlify": "./bin/run"
  },
  "bugs": "https://github.com/netlify/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.1.0",
    "ascii-table": "0.0.9",
    "chalk": "^2.4.1",
    "cli-ux": "^4.6.3",
    "configstore": "github:bcomnes/configstore#custom-path",
    "is-docker": "^1.1.0",
    "lodash.snakecase": "^4.1.1",
    "netlifys_api_definition": "^0.1.0",
    "open": "0.0.5",
    "opn": "^5.3.0",
    "util.promisify-all": "^1.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.31",
    "ava": "1.0.0-beta.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/netlify/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "netlify-cli",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "init": [
        "./src/hooks/header"
      ],
      "analytics": [
        "./src/hooks/analytics",
        "./src/hooks/hooks-two"
      ],
      "login": "./src/hooks/login"
    }
  },
  "repository": "netlify/cli",
  "scripts": {
    "docs": "export DOCS_GEN=TRUE && oclif-dev readme --multi && export DOCS_GEN=FALSE",
    "test": "run-s test:*",
    "test:lint": "eslint src",
    "test:ava": "ava src",
    "build": "rimraf dist && mkdirp dist && babel src --out-dir dist",
    "watch": "rimraf dist && mkdirp dist && babel src --out-dir dist --watch",
    "prepare": "npm run build"
  }
}
