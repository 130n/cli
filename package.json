{
  "name": "netlify-cli",
  "version": "0.0.0",
  "description": "CLI to interact with Netlify",
  "author": "davidwells @DavidWells",
  "bin": {
    "ntl": "./bin/run",
    "netlify": "./bin/run"
  },
  "bugs": "https://github.com/netlify/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.1.0",
    "ascii-table": "0.0.9",
    "chalk": "^2.4.1",
    "cli-ux": "^4.6.3",
    "configstore": "github:bcomnes/configstore#custom-path",
    "is-docker": "^1.1.0",
    "lodash.snakecase": "^4.1.1",
    "netlifys_api_definition": "^0.1.0",
    "open": "0.0.5",
    "opn": "^5.3.0",
    "util.promisify-all": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@oclif/dev-cli": "^1.13.31",
    "ava": "1.0.0-beta.6",
    "babel-eslint": "^8.2.5",
    "eslint": "^4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "sitedown": "^3.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/netlify/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "netlify-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/header"
      ],
      "analytics": [
        "./dist/hooks/analytics",
        "./dist/hooks/hooks-two"
      ],
      "login": "./dist/hooks/login"
    }
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "compileEnhancements": true,
    "require": [
      "@babel/register"
    ]
  },
  "repository": "netlify/cli",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "eslint src/util",
    "test:ava": "ava -vvvv",
    "build": "npm run clean && run-s build:*",
    "build:babel": "npm run clean && babel src --out-dir dist",
    "build:docs": "DOCS_GEN=TRUE oclif-dev readme --multi",
    "build:site": "sitedown . -b site/ -l layout.html",
    "clean": "rimraf dist site && mkdirp dist site",
    "watch": "run-p watch:*",
    "watch:babel": "npm run build:babel -- '--watch'",
    "watch:ava": "npm run test:ava -- '--watch'",
    "prepare": "npm run build",
    "start": "node ./bin/run",
    "ci": "run-s test build"
  }
}
