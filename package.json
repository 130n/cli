{
  "name": "netlify-cli",
  "description": "CLI to interact with Netlify",
  "version": "0.0.0",
  "author": "davidwells @DavidWells",
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": false,
    "compileEnhancements": true
  },
  "bin": {
    "ntl": "./bin/run",
    "netlify": "./bin/run"
  },
  "files": [
    "/bin",
    "/src"
  ],
  "bugs": "https://github.com/netlify/cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.1.2",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.1.0",
    "ascii-table": "0.0.9",
    "chalk": "^2.4.1",
    "cli-ux": "^4.6.3",
    "configstore": "github:bcomnes/configstore#custom-path",
    "dot-prop": "^4.2.0",
    "find-up": "^3.0.0",
    "flush-write-stream": "^1.0.3",
    "folder-walker": "^3.1.0",
    "hasha": "^3.0.0",
    "is-docker": "^1.1.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.snakecase": "^4.1.1",
    "make-dir": "^1.3.0",
    "netlifys_api_definition": "^0.1.0",
    "open": "0.0.5",
    "opn": "^5.3.0",
    "p-all": "^1.0.0",
    "p-limit": "^2.0.0",
    "p-map": "^1.2.0",
    "p-timeout": "^2.0.1",
    "p-wait-for": "^1.0.0",
    "parallel-transform": "^1.1.0",
    "pump": "^3.0.0",
    "through2-filter": "^3.0.0",
    "through2-map": "^3.0.0",
    "util.promisify": "^1.0.0",
    "util.promisify-all": "^1.0.2",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.31",
    "ava": "1.0.0-beta.6",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "sitedown": "^3.3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/netlify/cli",
  "keywords": [
    "api",
    "cli",
    "netlify",
    "static"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "netlify-cli",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "prerun": [
        "./src/hooks/prerun"
      ],
      "analytics": [
        "./src/hooks/analytics",
        "./src/hooks/hooks-two"
      ]
    }
  },
  "repository": "netlify/cli",
  "scripts": {
    "build": "npm run clean && run-s build:*",
    "build:docs": "DOCS_GEN=TRUE oclif-dev readme --multi",
    "build:site": "sitedown . -b site/ -l layout.html",
    "ci": "run-s test build",
    "clean": "rimraf site && mkdirp site",
    "start": "node ./bin/run",
    "test": "run-s test:*",
    "test:ava": "ava -vvvv",
    "test:lint": "eslint src",
    "watch": "run-p watch:*",
    "watch:ava": "npm run test:ava -- --watch"
  }
}
